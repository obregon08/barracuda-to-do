  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "devInspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "sb": "start-storybook -s ./public -p 9001",
    "build-sb": "build-storybook",
    "predeploy": "npm run clean && next build",
    "lint": "eslint . --ext .jsx,.ts,.tsx --config .eslintrc.json --fix",
    "lint:watch": "esw -w --changed --ext .jsx,.ts,.tsx,.ts --watch-delay 100 --fix",
    "pretty": "prettier --write .",
    "precommit": "lint-staged && yarn type-check",
    "type-check": "tsc -p tsconfig.json",
    "test": "cypress open",
    "x": "time yarn pretty && time yarn eslint --fix && time yarn type-check",
    "build-storybook": "build-storybook -s ./public",
    "chromatic": "yarn build-storybook -s ./public && chromatic --project-token 4bf426db6949 --exit-zero-on-changes "
